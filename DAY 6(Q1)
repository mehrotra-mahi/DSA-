void moveZeroes(int* nums, int numsSize) {
    int j = 0;  

    // move all non-zero elements to the front
    for (int i = 0; i < numsSize; i++) {
        if (nums[i] != 0) {
            nums[j] = nums[i];
            j++;
        }
    }

    // fill remaining positions with zero
    for (int i = j; i < numsSize; i++) {
        nums[i] = 0;
    }
}
